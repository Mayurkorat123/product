{
  "name": "puja",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build",
    "build:csr": "ng build --configuration csr",
    "dev:ssr": "ng serve",
    "dev:csr": "ng serve --configuration csr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:puja": "node dist/puja/server/server.mjs",
    "build:apk": "ng build --configuration production --output-path=dist && npx cap sync && cd android && gradlew assembleDebug"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.8",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "express": "^5.1.0",
    "ng-zorro-antd": "^20.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ws": "^8.18.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/ws": "^8.18.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
